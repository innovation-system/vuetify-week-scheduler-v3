{
  "name": "@innovation-system/vuetify-week-scheduler-v3",
  "version": "0.1.5",
  "description": "Vuetify week scheduler for Vue 3",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/vuetify-week-scheduler-v3.umd.cjs",
  "module": "./dist/vuetify-week-scheduler-v3.js",
  "exports": {
    ".": {
      "import": "./dist/vuetify-week-scheduler-v3.js",
      "require": "./dist/vuetify-week-scheduler-v3.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innovation-system/vuetify-week-scheduler-v3.git"
  },
  "bugs": {
    "url": "https://github.com/innovation-system/vuetify-week-scheduler-v3/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.build.config.js",
    "build:demo": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "release": "read -p 'GITHUB_TOKEN: ' GITHUB_TOKEN && export GITHUB_TOKEN=$GITHUB_TOKEN && release-it"
  },
  "dependencies": {
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@mdi/font": "^7.4.47",
    "@release-it/conventional-changelog": "^10.0.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "eslint-plugin-vuetify": "^2.5.2",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "release-it": "^19.0.1",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-eslint-parser": "^10.1.3"
  },
  "peerDependencies": {
    "@mdi/font": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "tagName": "v${version}",
      "requireBranch": "main",
      "requireCleanWorkingDir": false,
      "commitMessage": "chore(release): ${version}"
    },
    "hooks": {
      "before:init": [
        "npm run build"
      ]
    },
    "npm": {
      "publish": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "keywords": [
    "vue",
    "vuetify",
    "vuetify-week-scheduler",
    "scheduler",
    "week"
  ],
  "author": {
    "name": "Innovation-System",
    "email": "daniel@innovation-system.it",
    "url": "http://innovation-system.it"
  },
  "contributors": [
    {
      "name": "Mattia Thiella",
      "email": "reekoz@gmail.com"
    },
    {
      "name": "Daniel Lando",
      "email": "daniel.sorridi@gmail.com"
    }
  ],
  "engines": {
    "node": ">= 18"
  },
  "homepage": "https://github.com/innovation-system/vuetify-week-scheduler-v3/",
  "license": "MIT"
}
